<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head> 
<title>SiteDrive - {PAGE_TITLE}</title>
<meta http-equiv="Content-Type" content="text/html; charset={PAGE_ENCODING}">
<link href="css/styles.css" rel="stylesheet" type="text/css">
<script src="js/jquery-1.11.3.min.js"></script>
<script src="js/jquery.contextMenu.js" type="text/javascript"></script>
<link href="css/jquery.contextMenu.css" rel="stylesheet" type="text/css" />

<!-- arcticModal -->
<script src="js/jquery.arcticmodal-0.3.min.js"></script>
<link rel="stylesheet" href="css/jquery.arcticmodal-0.3.css">

<!-- arcticModal theme -->
<link rel="stylesheet" href="css/themes/simple.css">

<script src="script.js"></script>
<script src="js/ZeroClipboard.js"></script>
<base target="_self">
</head>
<body marginheight="0" marginwidth="0" rightmargin="0" leftmargin="0" topmargin="0" bottommargin="0">
<table width="100%" border="0" cellspacing="0" cellpadding="0" height="100%">
<tr height="35">
    <td background="images/header_bg.gif" style="border-bottom: solid 1px white">

		<table width="100%" height="100%" cellpadding="0" cellspacing="0" border="0">
		<tr>
			<td><img src="images/logo.jpg" width="133" height="35" border="0" alt="SiteDrive"></td>
			<td align="right" width="32">{LOGOUT_BUTTON}</td>
			<td align="right" width="32">{SITE_BUTTON}</td>
		</tr>
		</table>

	</td>
</tr>
<tr height="25">
  	<td style="border-top: solid 2px #FEB82F; border-bottom: solid 2px #FEB82F" background="images/main_menu_bg.gif">
	
    	<table cellpadding="0" cellspacing="0" border="0"  style="margin: 5px 15px">
		<tr>
			{MAIN_MENU}
		<tr>
		</table>
	
    </td>
</tr>
<tr>
  	<td valign="top" id="main_table">
  		{PAGE_CONTENT}
  	</td>
</tr>
<tr height="15">
	<td style="padding-right: 10px; border-top: solid 2px #FEB82F; text-align: right; font: normal 11px Tahoma">&laquo;SiteDrive&raquo; v3.0 (c) build 16.07.2015</td>
</tr>
</table>
</body>
</html>